<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="450.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Service Type" />
      <Label fx:id="serviceTypeLabel" text="[Service Type]" />
      <Label text="Service Description">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="serviceDescriptionLabel" text="[Service Description]" wrapText="true" />
      <Label text="Service Fee">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="serviceFeeLabel" text="[Service Fee]" />
      <GridPane hgap="7.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="NEVER" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <VBox.margin>
            <Insets bottom="7.0" top="15.0" />
         </VBox.margin>
         <children>
            <TextField fx:id="servicePaymentText" promptText="Service Payment Here" />
            <Button mnemonicParsing="false" onAction="#acceptPaymentAction" text="Accept Payment" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="30.0" left="25.0" right="25.0" top="30.0" />
   </padding>
</VBox>
