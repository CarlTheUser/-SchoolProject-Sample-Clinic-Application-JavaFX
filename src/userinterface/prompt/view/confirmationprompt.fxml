<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="130.0" minWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <HBox nodeOrientation="RIGHT_TO_LEFT" spacing="7.0" GridPane.rowIndex="1">
         <children>
            <Button fx:id="confirmationAction" defaultButton="true" mnemonicParsing="false" text="Ok" />
            <Button fx:id="cancellationAction" cancelButton="true" mnemonicParsing="false" text="Cancel" />
         </children>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
         <padding>
            <Insets bottom="7.0" left="7.0" right="7.0" top="15.0" />
         </padding>
      </HBox>
      <VBox>
         <children>
            <Label fx:id="promptMessageLabel" text="Label" />
         </children>
         <padding>
            <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
         </padding>
      </VBox>
   </children>
</GridPane>
